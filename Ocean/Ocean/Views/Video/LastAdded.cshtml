@model System.Collections.Generic.List<Ocean.Models.Video>

@{
    ViewData["Title"] = "New videos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron">
    <div class="display-4">@ViewData["Title"]</div>
    <hr class="my-4">
    <div class="col-sm-12 col-lg-12">
        @foreach (var video in Model)
        {

            <div class="card text-white bg-success col-sm-2 float-left mr-3 mt-3">
                <img src="~/images/Videos/robert-bye-55418-unsplash.jpg" class="img-fluid img-thumbnail rounded mt-2" />
                <h5 class="card-title mt-1">@video.Title</h5>
            </div>
        }
    </div>
</div>
